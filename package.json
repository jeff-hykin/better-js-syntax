{
    "name": "better-js-syntax",
    "displayName": "Better JavaScript Syntax",
    "description": "",
    "version": "0.0.2",
    "icon": "icon.png",
    "scripts": {},
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "repository": {
		"type": "git",
		"url": "https://github.com/jeff-hykin/better-js-syntax.git"
	},
    "contributes": {
        "grammars": [
            {
                "language": "javascript",
                "scopeName": "source.js",
                "path": "./autogenerated/js.tmLanguage.json",
                "embeddedLanguages": {
                    "text.html.basic.js": "html"
                }
            },
            {
                "language": "html",
                "scopeName": "text.html.basic",
                "path": "./autogenerated/html.tmLanguage.json",
                "embeddedLanguages": {
                    "source.css": "css",
                    "text.html.basic": "html",
                    "text.html.basic#template-html": "html",
                    "source.js": "javascript",
                    "source.python": "python",
                    "source.smarty": "smarty"
                },
                "tokenTypes": {
                    "meta.tag string.quoted": "other"
                }
            }
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "textmate-bailout": "^1.1.0",
        "vsce": "^1.99.0",
        "textmate-tester": "^1.1.0"
    }
}
